{
  "name": "node-boilerplate-ts",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run prod",
    "build": "npm-run-all clean transpile",
    "server": "node ./dist-server/bin/www",
    "dev": "cross-env NODE_ENV=dev npm-run-all build server",
    "prod": "cross-env NODE_ENV=prod npm-run-all build server",
    "transpile": "babel ./server --extensions .ts --out-dir dist-server --source-maps",
    "clean": "rimraf dist-server",
    "watch:dev": "nodemon -e ts,json --exec \"npm run dev\""
  },
  "dependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "@langchain/community": "^0.0.27",
    "@qdrant/js-client-rest": "^1.7.0",
    "@xenova/transformers": "^2.15.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "debug": "~2.6.9",
    "dotenv": "^16.3.1",
    "express": "~4.16.1",
    "express-validator": "^7.0.1",
    "langchain": "^0.1.17",
    "morgan": "~1.9.1",
    "multer": "^1.4.5-lts.1",
    "pdf-parse": "^1.1.1",
    "rimraf": "^5.0.5"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-typescript"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime",
      "source-map-support"
    ],
    "sourceMaps": "inline"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.23.6",
    "@babel/preset-typescript": "^7.23.3",
    "@babel/runtime": "^7.23.6",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/debug": "^4.1.12",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.17",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-source-map-support": "^2.2.0",
    "nodemon": "^3.0.2",
    "npm-run-all": "^4.1.5",
    "source-map-support": "^0.5.21",
    "typescript": "^5.3.3"
  }
}
